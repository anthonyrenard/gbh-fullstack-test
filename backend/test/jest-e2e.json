{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@vehicle/(.*)$": "<rootDir>/../src/vehicle/$1",
    "^@app/(.*)$": "<rootDir>/../src/app/$1",
    "^@src/(.*)$": "<rootDir>/../src/$1"
  }
}
